%% rt-propagators
\def\rrarc(#1,#2)(#3,#4,#5){
  \DoubleLine(
    #1 #3 4 sub #4 #5 #4 sub abs 2 div add cos mul add,
    #2 #3 4 sub #4 #5 #4 sub abs 2 div add sin mul add
    )(
    #1 #3 4 add #4 #5 #4 sub abs 2 div add cos mul add,
    #2 #3 4 add #4 #5 #4 sub abs 2 div add sin mul add
  ){1.5}
}
\def\rrline(#1,#2)(#3,#4){
%  \Vertex(#1 #3 #1 sub 2 div add,#2 #4 #2 sub 2 div add){2}
  \DoubleLine(
%    #1 #3 #1 sub abs 2 div add,
%    #2 #4 #2 sub abs 2 div add
    #1 #3 #1 sub 2 div add 4 #4 #2 sub #3 #1 sub atan 90 add cos mul add,
    #2 #4 #2 sub 2 div add 4 #4 #2 sub #3 #1 sub atan 90 add sin mul add
    )(
    #1 #3 #1 sub 2 div add 4 #4 #2 sub #3 #1 sub atan 90 add cos mul sub,
    #2 #4 #2 sub 2 div add 4 #4 #2 sub #3 #1 sub atan 90 add sin mul sub
  ){1.5}
}
\def\rarc(#1,#2)(#3,#4,#5){%
  {\SetArrowStroke{0.6}
  \Arc[arrow,arrowpos=0.5,arrowscale=0.8](#1,#2)(#3,
    #4 #5 #4 sub 2 div add 1 sub,
    #4 #5 #4 sub 2 div add 1 add)
}}
\def\aarc(#1,#2)(#3,#4,#5){%
  {\SetArrowStroke{0.6}
  \Arc[arrow,arrowpos=0.5,flip,arrowscale=0.8](#1,#2)(#3,
    #4 #5 #4 sub 2 div add 1 sub,
    #4 #5 #4 sub 2 div add 1 add)
}}

\def\rline(#1,#2)(#3,#4){%
  {\SetArrowStroke{0.6}
  \Line[arrow,arrowpos=0.5,arrowscale=0.8](
    #1 #3 #1 sub 2 div add 1 #4 #2 sub #3 #1 sub atan cos mul sub,
    #2 #4 #2 sub 2 div add 1 #4 #2 sub #3 #1 sub atan sin mul sub
    )(
    #1 #3 #1 sub 2 div add 1 #4 #2 sub #3 #1 sub atan cos mul add,
    #2 #4 #2 sub 2 div add 1 #4 #2 sub #3 #1 sub atan sin mul add
)}}


